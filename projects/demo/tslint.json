{
    "extends": "../../tslint.json",
    "rules": {
        "align": [
            true,
            "statements",
            "members",
            "elements"
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "allow-leading-underlevel"
        ],
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}